<div
  class="p-4 tab-pane fade {{#if active.all_cars}}show active{{/if}}"
  id="all-tab-pane"
  role="tabpanel"
  aria-labelledby="all-tab"
  tabindex="3"
>
  <h2 class="h2 text-center">All Cars</h2>
  <table class="table table-striped align-middle">
    <thead>
      <tr>
        <th scope="col">Car Number</th>
        <th scope="col">Slot No.</th>
        <th scope="col">Time of Parking</th>
      </tr>
    </thead>
    <tbody>
      {{#each all_vehicles as |slot s_no|}}
        <tr>
          <td>{{slot.car.registration_number}}</td>
          <td>{{slot.slot_no}}</td>
          <td>{{slot.timestamp}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>